{
	"name": "vscodevi",
	"description": "vi like extension",
	"version": "0.0.1",
	"publisher": "74th",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/src/extension",
	"contributes": {
		"keybindings": [
			{
				"command": "vim.h",
				"key": "h",
				"mac": "h",
				"when": "editorTextFocus && !suggestWidgetVisible"
			},
			{
				"command": "vim.i",
				"key": "i",
				"mac": "i",
				"when": "editorTextFocus && !suggestWidgetVisible"
			},
			{
				"command": "vim.j",
				"key": "j",
				"mac": "j",
				"when": "editorTextFocus && !suggestWidgetVisible"
			},
			{
				"command": "vim.k",
				"key": "k",
				"mac": "k",
				"when": "editorTextFocus && !suggestWidgetVisible"
			},
			{
				"command": "vim.l",
				"key": "l",
				"mac": "l",
				"when": "editorTextFocus && !suggestWidgetVisible"
			},
			{
				"command": "vim.esc",
				"key": "escape",
				"mac": "escape",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	}
}