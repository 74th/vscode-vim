{
	"name": "vscodevi",
	"description": "vi like extension",
	"version": "0.0.1",
	"publisher": "74th",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/src/extension",
	"contributes": {
		"keybindings": [{
			"command":"likevi.i",
			"key": "i",
			"mac": "i",
			"when": "editorTextFocus"
		},{
			"command":"likevi.j",
			"key": "j",
			"mac": "j",
			"when": "editorTextFocus"
		},{
			"command":"likevi.k",
			"key": "k",
			"mac": "k",
			"when": "editorTextFocus"
		},{
			"command":"likevi.esc",
			"key": "escape",
			"mac": "escape",
			"when": "editorTextFocus"
		}]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	}
}